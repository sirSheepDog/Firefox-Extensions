{
    "manifest_version": 2,
    "name": "DigiKey Search",
    "version": "1.0",
    "description": "Adds DigiKey to the list of search engines",
    "author": "Chris Arrandale",
	
	"icons": {
		"96": "DigiKeyIcon96.png"
	},

    "chrome_settings_overrides": {
        "search_provider": {
            "name": "DigiKey",
            "search_url": "https://www.digikey.com/en/products?keywords={searchTerms}",
            "keyword": ["@digi, @digikey"],
            "favicon_url": "https://www.digikey.com/favicon.ico",
            "is_default": false,
            "encoding": "UTF-8"
        }
    }
}