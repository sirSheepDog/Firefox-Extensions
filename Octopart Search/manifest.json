{
    "manifest_version": 2,
    "name": "Octopart Search",
    "version": "1.0",
    "description": "Adds Octopart to the list of search engines",
    "author": "Chris Arrandale",
	
	"icons": {
		"96": "OctopartIcon96.png"
	},

    "chrome_settings_overrides": {
        "search_provider": {
            "name": "Octopart",
            "search_url": "https://octopart.com/search?q={searchTerms}",
            "keyword": ["@octo","@octopart"],
            "favicon_url": "https://octopart.com/favicon.ico",
            "is_default": false,
            "encoding": "UTF-8"
        }
    }
}